<div class="card" id="card<%= @card.id %>">
  <%= link_to(game_cast_votes_path(
    game_id: @game.id,
    voter_id: current_user.id,
    play_id: @card.plays.find_by(round_id: @game.this_round.id),
    card_id: @card.id
  ), method: :post, remote: true) do %>

    <h3><%= @card.name %></h3>
    <p><%= @card.bio %></p>

  <% end %>
</div>